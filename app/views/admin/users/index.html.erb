<% provide :title, t(".manage_users") %>
<% provide :placeholder, t(".search_user") %>
<h1><%= t (".manage_users") %></h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag admin_users_path, method: :get do %>
      <%= render "shared/search" %>
    <% end %>
  </div>
</div>
<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th>#</th>
        <th><%= t "name" %></th>
        <th><%= t "uploaded_song" %></th>
        <th><%= t "action" %></th>
      </tr>
    </thead>
    <tbody class="list-table">
      <%= render @users %>
    </tbody>
  </table>
  <%= will_paginate %>
</div>
